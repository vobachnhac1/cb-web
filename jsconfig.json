{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "*"
      ]
    }
  },
  "include": [
    // "next-env.d.ts",
    "**/*.js",
    "**/*.jsx", "src/components/BurgerIcon",
    // "src/components/room-client/index.jsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "moduleResolution": [
    "node_modules",
    ".next",
    "node"
  ]
}